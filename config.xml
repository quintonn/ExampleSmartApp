<?xml version='1.0' encoding='utf-8'?>
<widget id="com.lyniate.qms" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Lyniate QMS</name>
    <description>
        Lyniate sample queue management system to show off functionality of Smart on FHIR.
    </description>
    <author email="quintonn.rothmann@lyniate.com" href="http://cordova.io">
        Quintonn Rothmann @ Lyniate
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="mycoolapp:" />
    <allow-intent href="mycoolapp:*" />
    <allow-intent href="mycoolapp://*/*" />
    <access origin="mycoolapp://*" launch-external="yes" />
    <allow-navigation href="mycoolapp://*/*" />
    <allow-navigation href="mycoolapp://*" />
    <allow-navigation href="mycoolapp:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
</widget>
